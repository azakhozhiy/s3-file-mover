{
  "name": "azakhozhiy/s3-files-mover",
  "description": "Console application for moving files from one S3 to other S3.",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Azk\\S3FileMover\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Aleksandr Zakhozhiy",
      "email": "azk@avzare.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0",
    "aws/aws-sdk-php": "^3.215",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "scripts": {
    "csf": [
      "./vendor/bin/php-cs-fixer fix src --using-cache=no --allow-risky=yes"
    ]
  }
}
